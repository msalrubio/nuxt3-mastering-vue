{
  "root": true,
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:vue/vue3-recommended",
    "@vue/prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "plugins": [],
  "rules": {
    "no-console": "warn",
    "semi": "error",
    "vue/no-multiple-template-root": "off",
    "vue/multi-word-component-names": "off"
  }
}
